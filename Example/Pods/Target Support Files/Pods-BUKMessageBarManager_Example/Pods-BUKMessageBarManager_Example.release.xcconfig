FRAMEWORK_SEARCH_PATHS = $(inherited) "$PODS_CONFIGURATION_BUILD_DIR/BUKMessageBarManager" "$PODS_CONFIGURATION_BUILD_DIR/BlocksKit"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited) -iquote "$PODS_CONFIGURATION_BUILD_DIR/BUKMessageBarManager/BUKMessageBarManager.framework/Headers" -iquote "$PODS_CONFIGURATION_BUILD_DIR/BlocksKit/BlocksKit.framework/Headers"
OTHER_LDFLAGS = $(inherited) -framework "BUKMessageBarManager" -framework "BlocksKit"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
